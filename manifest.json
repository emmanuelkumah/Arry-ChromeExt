{
  "manifest_version": 3,
  "name": "Arry",
  "description": "Learn and improve on your JavaScript Array method whenever you open a new tab",
  "version": "1.0.1",
  "icons": {
    "48": "icons/fav.png",
    "64": "icons/fav.png"
  },
  "action": {
    "default_title": "Learn Arrays Easily",
    "default_icon": "icons/fav.png",
    "default_popup": "popup/popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
